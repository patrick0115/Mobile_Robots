<launch>
<!-- run serial node for base mega -->    
	<node pkg="cp3" type="serial_node.py" name="serial_node" output="screen">
            <param name="port" value="/dev/ttyACM0"/>
            <param name="baud" value="57600"/>
	 </node>
       <node name="cp3" pkg="cp3" type="cp3.py" output="screen" />
   	
</launch>
